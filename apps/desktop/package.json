{
  "name": "@promptx/desktop",
  "version": "1.9.0",
  "description": "PromptX Desktop - Local AI prompt management application",
  "author": {
    "name": "Deepractice",
    "email": "sean@deepracticex.com"
  },
  "type": "module",
  "main": "out/src/main.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "npm run build:watch & npm run start:electron",
    "dev:vite": "electron-vite dev",
    "build": "rimraf out && tsc",
    "build:watch": "tsc --watch",
    "start:electron": "wait-on out/src/main.js && electron .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist out release",
    "package": "pnpm build && electron-builder",
    "package:mac": "pnpm build && electron-builder --mac",
    "package:win": "pnpm build && electron-builder --win",
    "package:linux": "pnpm build && electron-builder --linux",
    "dist": "pnpm package:mac"
  },
  "dependencies": {
    "@promptx/cli": "workspace:*",
    "fastmcp": "^3.14.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "electron": "28.3.3",
    "electron-builder": "^26.0.12",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "wait-on": "^7.2.0"
  }
}